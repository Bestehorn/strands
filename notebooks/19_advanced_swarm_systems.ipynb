    "class RAGEnhancedAgent:\n",
    "    \"\"\"Agent wrapper that provides RAG context for queries\"\"\"\n",
    "    \n",
    "    def __init__(self, agent: Agent, rag_system: EnterpriseRAGSystem, agent_name: str):\n",
    "        self.agent = agent\n",
    "        self.rag_system = rag_system\n",
    "        self.agent_name = agent_name\n",
    "        \n",
    "    def __call__(self, query: str, include_context: bool = True) -> str:\n",
    "        \"\"\"Process query with optional RAG context\"\"\"\n",
    "        if include_context:\n",
    "            # Get relevant document context\n",
    "            context = self.rag_system.get_context_for_query(query)\n",
    "            enhanced_query = f\"{context}\\n\\nBased on the above documents, {query}\"\n",
    "            logger.info(f\"{self.agent_name} processing query with RAG context\")\n",
    "        else:\n",
    "            enhanced_query = query\n",
    "            logger.info(f\"{self.agent_name} processing query without RAG context\")\n",
    "            \n",
    "        response = self.agent(enhanced_query)\n",
    "        return str(response)\n",
    "\n",
