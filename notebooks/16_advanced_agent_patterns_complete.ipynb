    "def simulate_customer_interactions(router: LearningRouter, num_interactions: int = 20):\n",
    "    \"\"\"Simulate customer interactions to demonstrate learning\"\"\"\n",
    "    \n",
    "    # Sample queries with expected patterns\n",
    "    sample_queries = [\n",
    "        (\"Where is my order?\", \"order_status\", 5),\n",
    "        (\"Track package #54321\", \"order_status\", 5),\n",
    "        (\"Product not working\", \"technical_issue\", 4),\n",
    "        (\"Screen is broken\", \"technical_issue\", 5),\n",
    "        (\"Refund request\", \"billing_issue\", 5),\n",
    "        (\"Double charged\", \"billing_issue\", 5),\n",
    "        (\"What are your hours?\", \"general_question\", 5),\n",
    "        (\"Company policy?\", \"general_question\", 4),\n",
    "    ]\n",
    "    \n",
    "    print(\"ðŸ”„ Simulating Customer Interactions\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    for i in range(num_interactions):\n",
    "        # Pick a random query\n",
    "        import random\n",
    "        query_text, expected_pattern, satisfaction = random.choice(sample_queries)\n",
    "        customer_id = f\"SIM{i:03d}\"\n",
    "        \n",
    "        # Route query\n",
    "        result = router.route_query(customer_id, query_text)\n",
    "        \n",
    "        # Simulate satisfaction (higher if routed correctly)\n",
    "        if result['pattern'] == expected_pattern:\n",
    "            actual_satisfaction = satisfaction\n",
    "        else:\n",
    "            actual_satisfaction = max(1, satisfaction - 2)\n",
    "        \n",
    "        # Update metrics\n",
    "        router.update_routing_metrics(\n",
    "            result['pattern'],\n",
    "            result['specialist'],\n",
    "            actual_satisfaction\n",
    "        )\n",
    "        \n",
    "        if i % 5 == 0:\n",
    "            print(f\"\\nAfter {i+1} interactions:\")\n",
    "            print(f\"Query: '{query_text}' â†’ {result['specialist']} (confidence: {result['confidence']:.2f})\")\n",
    "    \n",
    "    # Show final routing performance\n",
    "    print(\"\\nðŸ“Š Final Routing Performance\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    performance = router.db.get_routing_performance()\n",
    "    for metric in performance[:5]:  # Top 5\n",
    "        print(f\"{metric['query_pattern']} â†’ {metric['specialist']}: \")\n",
    "        print(f\"  Success Rate: {metric['success_rate']:.2%} ({metric['total_count']} queries)\")\n",
    "        print(f\"  Avg Satisfaction: {metric['avg_satisfaction']:.1f}/5\")\n",
    "\n",
    "# Run simulation\n",
    "simulate_customer_interactions(router)"
